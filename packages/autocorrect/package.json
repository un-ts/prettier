{
  "name": "prettier-plugin-autocorrect",
  "version": "0.4.3",
  "type": "module",
  "description": "An opinionated AutoCorrect plugin for Prettier",
  "repository": "git+https://github.com/un-ts/prettier.git",
  "homepage": "https://github.com/un-ts/prettier/tree/master/packages/autocorrect",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/unts",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./index.d.cts",
  "module": "./lib/index.js",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "index.d.cts",
    "lib"
  ],
  "keywords": [
    "autocorrect",
    "copywriting",
    "spaces",
    "punctuations",
    "CJK"
  ],
  "peerDependencies": {
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "autocorrect-node": "^2.13.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
