{
  "name": "@unts/prettier",
  "version": "0.0.0",
  "type": "module",
  "description": "Opinionated but Incredible Prettier plugins.",
  "repository": "git+https://github.com/un-ts/prettier.git",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "build": "run-p 'build:*'",
    "build:r": "r -f cjs -e named",
    "build:ts": "tsc -b",
    "clean": "rimraf dist coverage 'packages/*/{lib,*.tsbuildinfo}'",
    "dev": "vitest",
    "docs": "vite",
    "docs:build": "vite build",
    "format": "tsx scripts/format",
    "languages": "tsx scripts/languages",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache",
    "lint:tsc": "tsc --noEmit",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate && yarn languages || exit 0",
    "release": "changeset publish",
    "serve": "sirv dist",
    "test": "vitest run",
    "typecov": "type-coverage",
    "version": "changeset version && yarn --no-immutable"
  },
  "devDependencies": {
    "@1stg/browserslist-config": "^2.1.2",
    "@1stg/common-config": "^12.0.0",
    "@1stg/postcss-config": "^6.0.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@mdx-js/rollup": "^3.1.0",
    "@pkgr/rollup": "^6.0.2",
    "@shikijs/rehype": "^3.2.1",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/web": "^0.0.216",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "clean-pkg-json": "^1.2.1",
    "eslint": "^9.24.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "github-markdown-css": "^5.8.1",
    "linguist-languages": "^7.29.0",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-autocorrect": "workspace:*",
    "prettier-plugin-pkg": "workspace:*",
    "prettier-plugin-sh": "workspace:*",
    "prettier-plugin-sql": "workspace:*",
    "prettier-plugin-toml": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "simple-git-hooks": "^2.12.1",
    "sirv-cli": "^3.0.1",
    "size-limit": "^11.2.0",
    "size-limit-preset-node-lib": "^0.3.0",
    "tsx": "^4.19.3",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vitest": "^3.1.1",
    "yarn-berry-deduplicate": "^6.1.1"
  },
  "resolutions": {
    "es5-ext": "npm:@unes/es5-ext@latest",
    "prettier": "^3.5.3",
    "prettier-plugin-autocorrect": "workspace:*",
    "prettier-plugin-pkg": "workspace:*",
    "prettier-plugin-sh": "workspace:*",
    "prettier-plugin-sql": "workspace:*",
    "prettier-plugin-toml": "workspace:*"
  },
  "browserslist": [
    "extends @1stg/browserslist-config/modern"
  ],
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "markuplint": {
    "extends": "@1stg"
  },
  "size-limit": [
    {
      "path": "packages/autocorrect/lib/index.js",
      "limit": "540B"
    },
    {
      "path": "packages/pkg/lib/index.js",
      "limit": "450B"
    },
    {
      "path": "packages/sh/lib/index.js",
      "limit": "2.4kB"
    },
    {
      "path": "packages/sql/lib/index.js",
      "limit": "2.5kB"
    }
  ],
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreNested": true,
    "ignoreNonNullAssertion": true,
    "ignoreFiles": [
      "**/*.d.ts"
    ],
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
